{
  "name": "@cmi-dair/brainviewer",
  "displayName": "brainviewer",
  "description": "Brain viewer",
  "version": "0.0.2",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
