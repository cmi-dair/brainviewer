    "@rollup/plugin-typescript": "^11.1.2",
    "@types/d3": "^7.4.0",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/three": "^0.154.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
